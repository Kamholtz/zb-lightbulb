/ {
	chosen {
		zephyr,entropy = &rng;
	};


	pwmleds {
		pwm_led3: pwm_led_3 {
			pwms = <&pwm0 16>;
		};
		ext_pwm_led: pwm_led_4 {
			pwms = < &pwm0 0xa >;
		};
	};

    aliases {
		pwm-on-board = &pwm_led3;
		pwm-ext0 = &ext_pwm_led;
    };
};
